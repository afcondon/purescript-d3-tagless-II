{
  "name": "purescript-d3-dataviz",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "extract-snippets": "node scripts/extract-snippets.js",
    "build": "npm run extract-snippets && spago build",
    "build:selection": "spago build -p psd3-selection",
    "build:simulation": "spago build -p psd3-simulation",
    "build:music": "spago build -p psd3-music",
    "build:website": "spago build -p demo-website",
    "build:ce": "spago build -p ce-website",
    "build:ce-server": "spago build -p ce-server",
    "bundle:ce": "spago bundle -p ce-website --module Main --outfile public/bundle.js",
    "dev:ce": "npm run build:ce && npm run bundle:ce",
    "serve:ce": "npx http-server ce-website/public -p 1235",
    "serve:ce-api": "node ce-server/run.js",
    "load-ce-db": "node ce-database/loader/load-from-json.js",
    "clean-build": "rm -rf output/PSD3* && spago build",
    "install": "spago install",
    "upgrade-set": "spago upgrade-set",
    "convert-about": "node scripts/convert-about.js",
    "generate-spago-data": "node scripts/generate-spago-data.js",
    "copy-sources": "mkdir -p docs/sources && cp -r psd3-selection/src/PSD3 docs/sources/",
    "bundle": "spago bundle -p demo-website --module PSD3.Main --outfile ../docs/bundle.js",
    "docs:api": "spago docs --format html && node scripts/filter-api-docs.js",
    "docs:api:commit": "npm run docs:api && git add docs/api/ && echo 'API docs regenerated. Commit when library API changes significantly.'",
    "docs:api:open": "spago docs --format html --open",
    "docs:api:local": "npm run docs:api && open docs/api/index.html",
    "graph-dot": "spago graph modules --dot",
    "graph-json": "spago graph modules --json && spago graph packages --json",
    "graph-topo": "spago graph modules --topo",
    "versions": "spago --version && npm --version",
    "dev": "npm run build && npm run bundle",
    "serve": "npx http-server docs -p 1234"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "cbor": "^10.0.11",
    "cssnano": "^7.1.1",
    "http-server": "^14.1.1",
    "marked": "^16.4.1",
    "postcss-cli": "^11.0.1",
    "postcss-import": "^16.1.1",
    "postcss-nesting": "^13.0.2",
    "tailwindcss": "^3.4.16"
  },
  "dependencies": {
    "@anthropic-ai/claude-code": "^2.0.73",
    "claude": "^0.1.1",
    "d3-array": "^3.2.4",
    "d3-brush": "^3.0.0",
    "d3-chord": "^3.0.1",
    "d3-color": "^3.1.0",
    "d3-drag": "^3.0.0",
    "d3-ease": "^3.0.1",
    "d3-force": "^3.0.0",
    "d3-hierarchy": "^3.1.2",
    "d3-interpolate": "^3.0.1",
    "d3-sankey": "^0.12.3",
    "d3-scale": "^4.0.2",
    "d3-scale-chromatic": "^3.1.0",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.2.0",
    "d3-transition": "^3.0.1",
    "d3-zoom": "^3.0.0",
    "duckdb": "^1.4.2",
    "esbuild": "^0.25.10",
    "purescript": "^0.15.15",
    "purs-tidy": "^0.11.1",
    "spago": "^0.93.44"
  }
}
